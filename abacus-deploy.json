{
  "name": "TAMBO MCP Integration Suite",
  "version": "2.0.0",
  "domain": "filesinasnap.com",
  "build": {
    "command": "npm run build",
    "outputDirectory": "dist",
    "publicDirectory": "dist"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "environment": {
    "NODE_ENV": "production",
    "VITE_APP_NAME": "TAMBO MCP Integration Suite",
    "VITE_APP_VERSION": "2.0.0",
    "VITE_ABACUS_API_ENDPOINT": "https://api.abacus.ai/mcp",
    "VITE_TAMBO_API_URL": "https://api.tambo.dev"
  },
  "github": {
    "repository": "git@github.com:Greenmamba29/tambo-mcp-integration-suite.git",
    "branch": "main",
    "auto_deploy": true
  }
}

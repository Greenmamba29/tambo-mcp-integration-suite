{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "VITE_GOOGLE_GEMINI_API_KEY": "@vite_google_gemini_api_key",
    "VITE_TAMBO_API_KEY": "@vite_tambo_api_key",
    "VITE_TAMBO_API_BASE_URL": "@vite_tambo_api_base_url",
    "VITE_ABACUS_APP_ID": "@vite_abacus_app_id",
    "VITE_ABACUS_BASE_URL": "@vite_abacus_base_url"
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

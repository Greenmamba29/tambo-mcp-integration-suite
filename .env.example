# Google Gemini AI API Key for TAMBO Intelligence Layer
VITE_GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key_here

# TAMBO API Configuration
VITE_TAMBO_API_KEY=your_tambo_api_key_here
VITE_TAMBO_API_BASE_URL=https://api.tambo.co/v1

# ABACUS Configuration
VITE_ABACUS_APP_ID=your_abacus_app_id_here
VITE_ABACUS_BASE_URL=https://apps.abacus.ai/chatllm

# Supabase Configuration for AIndieStore Database
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Security & Encryption
VITE_ENCRYPTION_KEY=your-32-char-encryption-key-here

# App Configuration
VITE_APP_NAME="TAMBO MCP Integration Suite Enhanced"
VITE_APP_VERSION="2.0.0"
VITE_APP_ENVIRONMENT=development

# Performance & Monitoring
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_SECURITY_LOGGING=true
VITE_ANALYTICS_ENABLED=true

# File Upload Configuration
VITE_MAX_FILE_SIZE=104857600
VITE_ALLOWED_FILE_TYPES=image/*,video/*,audio/*,application/pdf,text/*
VITE_STORAGE_BUCKET=files

# MCP Serverless API Configuration (Vercel Environment Variables)
MCP_API_KEY=your_secret_mcp_api_key_for_bearer_auth
ABACUS_API_KEY=your_abacus_api_key_here
ABACUS_DEPLOYMENT_ID=your_chatllm_deployment_id
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_for_logging
